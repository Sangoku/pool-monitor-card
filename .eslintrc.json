{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": [
    "eslint:recommended",
    "prettier"
  ],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "globals": {
    "LitElement": "readonly"
  },
  "rules": {
    "no-unused-vars": "warn",
    "no-console": "off",
    "quotes": "off",
    "semi": "off",
    "indent": "off",
    "comma-dangle": "off",
    "no-undef": "error"
  }
}
